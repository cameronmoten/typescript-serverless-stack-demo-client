{
  "extends": [ "tslint-react"],
  "linterOptions": {
   
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "jsRules": {
    "object-literal-sort-keys": false,
    "jsx-wrap-multiline" : false,
    "jsx-boolean-value" : false,
    "jsx-no-multiline-js" : false
  }, 
  "rules": {
    "object-literal-sort-keys": false, //Disable for typescript
    "jsx-wrap-multiline" : false,
    "jsx-boolean-value" : false,
    "jsx-no-multiline-js" : false
  } 
}
    